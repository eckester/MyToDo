{
    "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
    ],
    "env": {
    "es2023": true
    },
    "parserOptions": {
    "sourceType": "module"
    },
    "rules": {
    "consistent-return": "error",
    "eqeqeq": "warn",
    "react/prop-types": 0,
    "react/react-in-jsx-scope": "off"
    },
    "settings": {
    "react": {
    "version": "detect"
    }
    }
    }
    